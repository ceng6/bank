       <script>
           //var $ = jQuery.noConflict();
               /*$(window).load(function() {
                   Grid.init();
               });*/

			$(function(){
				$("#clk").click(function(){
				var params = getCommonParams(document.cookie);
                params.class = "depsoitfabupricingList";
                params.RQ =$("#meeting").val();
                params.TX =$("#tx option:selected").val();
                params.JG1=params.S_organno;
                params.CP =$("#cp option:selected").val();
                  $.ajax({
                    url:port,
                    data:params,
                    dataType:'json',
                    cache:false,
                    success:function(r){
                          debugger;
                          if (r.status){
                              $.each(r.result,function(i,v){
                              $('<div class="content-wrapper-div">'
                                    +'<ul>'
                                      +'<li>计算日期：<span>'+v.as_of_date+'</span></li>'
                                      +'<li>机构号：<span>'+v.organno+'</span></li>'
                                      +'<li>机构名称：<span>'+v.organname+'</span></li>'
                                      +'<li>条线：<span>'+v.line_name+'</span></li>'
                                      +'<li>产品名称：<span>'+v.item+'</span></li>'
                                      +'<li>资金价值：<span>'+v.ftp_cost+'</span></li>'
                                      +'<li>流动性成本：<span>'+v.ld_batch+'</span></li>'
                                      +'<li>运营成本：<span>'+v.costs_rate_bm+'</span></li>'
                                      +'<li>存款保险：<span>'+v.ckbx_batch+'</span></li>'
                                      +'<li>目标收益率：<span>'+v.target_return+'</span></li>'
                                      +'<li>存款价格：<span>'+v.ck_cost+'</span></li>'
                                    +'</ul>'
                            +'</div>').appendTo('#content');
                         $(".wrapper-kong").css("height",0)

                        });
                        }else{
                            alert(r.message);
                          }
                    },error:function(r){
                      alert('用户没有权限！');
                    }
              });
		                     
            })
    })
